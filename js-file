var twoSum = function(nums, target) {
  let i=0;
  let j=0;
  let firstNum=0;
  let secoundNum=0;
  console.log(nums.length)
  while(i<=nums.length-1)
  {
    firstNum=nums[i]
    j=i+1;
    while(j<nums.length){
      secoundNum=nums[j]
      if(firstNum+secoundNum===target){
        console.log(i,j)
      }
      j++
    }
    i++
  
  }
    }

twoSum([2,7,11,15],13)